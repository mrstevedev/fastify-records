generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Record {
  id       String @id
  title    String
  artist   String
  label    String
  format   String
  country  String
  released String
  genre    String
  style    String
  price    String
  image    String
}

model User {
  id          Int @id @default(autoincrement())
  username    String @unique
  email       String @unique
  password    String
  first_name  String
  last_name   String
  country     String
  role        String
}